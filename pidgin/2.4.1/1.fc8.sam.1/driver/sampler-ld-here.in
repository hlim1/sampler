#!/bin/sh

driverdir=`dirname $0`
homedir=$driverdir/..

exec "$driverdir/sampler-cc-here" \
  "-L$homedir/lib/schemes/.libs" \
  "-L$homedir/lib/threads/yes/.libs" \
  "-L$homedir/lib/threads/no/.libs" \
  "-Wl,-rpath,$homedir/lib/schemes/.libs" \
  "-Wl,-rpath,$homedir/lib/threads/yes/.libs" \
  "-Wl,-rpath,$homedir/lib/threads/no/.libs" \
  "$@"
