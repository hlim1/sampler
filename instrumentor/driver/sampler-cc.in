#!/bin/sh

scheme=$1
if ! shift; then
    echo "Usage: $0 <scheme> [<compiler arguments> ...]" >&2
    exit 2
fi

PACKAGE='@PACKAGE@'

prefix='@prefix@'
exec_prefix="@exec_prefix@"
libdir="@libdir@"
package_libdir="@package_libdir@"
instrumentordir="@instrumentordir@"
driverdir="@driverdir@"

exec $driverdir/main $scheme "$@"
