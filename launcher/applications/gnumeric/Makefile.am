links =						\
	executable				\
	gdb-commands				\
	gnome-gnumeric.png			\
	print-debug-info

all-local: $(links)

executable: ; ln -s ../../../../gnumeric/cvs/branches/src/gnumeric $@

gdb-commands: ; ln -s ../../$@ .

gnome-gnumeric.png: ; ln -s /usr/share/pixmaps/$@ .

print-debug-info: ; ln -s ../../$@ .
